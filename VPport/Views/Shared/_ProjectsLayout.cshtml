<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>@ViewBag.Title</title>
    <link href="https://fonts.googleapis.com/css?family=Roboto:100,100i,300,300i,400,400i,500,500i,700,700i,900,900i" rel="stylesheet">
    <link href="~/content/css/layout.css" rel="stylesheet" />
    <link href="~/content/css/projects.css" rel="stylesheet" />
    <link rel="stylesheet" href="https://use.fontawesome.com/releases/v5.6.1/css/all.css" integrity="sha384-gfdkjb5BdAXd+lj+gudLWI+BXq4IuLW5IT+brZEZsLFm++aCMlF1V92rMkPaX4PP" crossorigin="anonymous">
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/2.1.3/jquery.min.js"></script>

</head>
<body>
    <div class="wrapper">
        <nav>
            <ul class="nav nav-tabs">
                <li role="presentation"><a href="/Home/Projects">Projects.</a></li>
                <li role="presentation"><a href="/Home/About">About.</a></li>
                <li role="presentation"><a href="/Home/Contact">Contact.</a></li>
            </ul>
        </nav>
        @RenderBody()
    </div>


    <script src="~/content/js/JavaScript.js"></script>
    <script>
        $(document).ready(function () {
            $(".preloader-wrapper-yellow").delay(2000).fadeOut(500);
            $(".preloader-wrapper-purple").delay(1500).fadeOut(500);
            $(".preloader-wrapper-red").delay(1200).fadeOut(500);
            $(".preloader-wrapper-blue").delay(1200).fadeOut(500);
        });
    </script>
</body>

</html>
